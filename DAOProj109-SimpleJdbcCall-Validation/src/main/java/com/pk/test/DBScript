// CREATE PROCEDURE

CREATE OR REPLACE PROCEDURE GET_PASSENGER_DETAILS (PID IN NUMBER, PNAME OUT VARCHAR2, MOB OUT NUMBER, SOURCE OUT VARCHAR2, DESTINATION OUT VARCHAR2)
AS
BEGIN
SELECT PSNGR_NAME,PSNGR_MOB,JOURNEY_SOURCE,JOURNEY_DESTINATION INTO PNAME,MOB,SOURCE,DESTINATION FROM TICKTE_RESERVATION WHERE PSNGR_ID = PID;
END;
